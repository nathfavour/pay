// API base URL for backend services
NEXT_PUBLIC_API_BASE_URL=https://api.example.com

// Default currency for the app
NEXT_PUBLIC_DEFAULT_CURRENCY=USD

// (Optional) API key for third-party services
# NEXT_PUBLIC_API_KEY=your-api-key-here

// (Optional) WebSocket endpoint for real-time updates
# NEXT_PUBLIC_WS_URL=wss://ws.example.com

// (Optional) Feature flags
# NEXT_PUBLIC_FEATURE_EXCHANGE=true
# NEXT_PUBLIC_FEATURE_REQUESTS=true

// (Optional) Analytics or Sentry DSN
# NEXT_PUBLIC_SENTRY_DSN=


